#In python
def largest(self, arr):
      return max(arr)
arr=list(map(int,input().split()))
largest(arr)



//In C
#include<stdio.h>
int largest(int arr[], int n) {
int max;
max=arr[0];
int i;
for(i=1;i<n;i++)
{
if(max<arr[i]){
max=arr[i];
}
}
return max;
}
